<text fixation>
	/items = fixation
	/select = 1
	/fontstyle = ("Arial", 80ptc)
	/txbgcolor = (0,0,0)
	/txcolor = (255,255,255)
</text>

<text correctmsg>
	/items = correctmsg
	/select = 1
	/fontstyle = ("楷体", 60ptc, false, false, false, false, 5, 1)
	/txcolor = (255,255,255)
	/txbgcolor = (0, 0, 0)
</text>

<text errormsg>
	/items = errormsg
	/select = 1
	/fontstyle = ("楷体", 60ptc, false, false, false, false, 5, 1)
	/txcolor = (255,255,255)
	/txbgcolor = (0, 0, 0)
</text>

<values>
	/fixationtime = 0
	/shapetime = 0
	/stimulustimep = 0
</values>

<expressions>
	/generaterandomfixation = ceil(rand(400,600))
	/generaterandomshape = ceil(rand(200,300))
</expressions>


<trial AR1>
	/pretrialpause = 500
	/validresponse = ("F","J")
	/correctresponse = ("F")
	/ontrialbegin = [values.fixationtime = expressions.generaterandomfixation]
	/ontrialbegin = [values.shapetime = values.fixationtime + expressions.generaterandomshape]
	/ontrialbegin = [values.stimulustimep = values.shapetime + 3000]
	/ontrialbegin = [trial.AR1.insertstimulustime(text.fixation, 0)]
	/ontrialbegin = [trial.AR1.insertstimulustime(shape.blank,values.fixationtime)]
	/ontrialbegin = [trial.AR1.insertstimulustime(picture.AR,values.shapetime)]
	/ontrialbegin = [trial.AR1.insertstimulustime(shape.blank,values.stimulustimep)]
	/ontrialend = [trial.AR1.clearstimulusframes()]
	/beginresponsetime = values.shapetime
</trial>

<trial BR1>
